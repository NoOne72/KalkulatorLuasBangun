package com.example.nyobapemmob; // <-- SESUAIKAN DENGAN PACKAGE KAMU

public class Shape {
    private String name;
    private int iconResId;

    public Shape(String name, int iconResId) {
        this.name = name;
        this.iconResId = iconResId;
    }

    public String getName() {
        return name;
    }

    public int getIconResId() {
        return iconResId;
    }
}
